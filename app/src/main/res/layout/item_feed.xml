<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingTop="@dimen/dp_20"
    xmlns:tools="http://schemas.android.com/tools">

    <TextView
        android:id="@+id/tvtime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:text="2023-02-25"
        android:textSize="@dimen/sp_12"
        android:textColor="@color/c_8a91a0"
        android:textStyle="bold"
        app:layout_constraintTop_toTopOf="parent" />
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        app:layout_constraintTop_toBottomOf="@id/tvtime"
        android:layout_marginTop="@dimen/dp_12"
        android:background="@drawable/shape8_18152a"
        android:layout_marginLeft="@dimen/dp_15"
        android:layout_marginRight="15dp"
        android:paddingTop="@dimen/dp_16"
        android:paddingLeft="@dimen/dp_12"
        android:paddingRight="@dimen/dp_12"
        android:paddingBottom="@dimen/dp_16"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/tv1"
            android:layout_width="wrap_content"
            android:textStyle="bold"
            android:textColor="@color/c_8a91a0"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            android:text="@string/txt_feedtype"
            android:textSize="@dimen/sp_14"
            android:layout_height="wrap_content"/>
        <TextView
            android:id="@+id/tv11"
            android:layout_width="wrap_content"
            android:textStyle="bold"
            android:textColor="@color/white"
            app:layout_constraintTop_toTopOf="@id/tv1"
            app:layout_constraintLeft_toRightOf="@id/tv1"
            android:layout_marginLeft="13dp"
            android:text="@string/txt_feedtype"
            android:textSize="@dimen/sp_14"
            android:layout_height="wrap_content"/>
        <TextView
            android:id="@+id/tv2"
            android:layout_width="wrap_content"
            android:textStyle="bold"
            android:textColor="@color/c_8a91a0"
            app:layout_constraintTop_toBottomOf="@id/tv1"
            app:layout_constraintLeft_toLeftOf="parent"
            android:text="@string/txt_feedcontent"
            android:layout_marginTop="@dimen/dp_11"
            android:textSize="@dimen/sp_14"
            android:layout_height="wrap_content"/>
        <TextView
            android:id="@+id/tv22"
            android:layout_width="0dp"
            android:textStyle="bold"
            android:textColor="@color/white"
            app:layout_constraintTop_toTopOf="@id/tv2"
            app:layout_constraintLeft_toRightOf="@id/tv2"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginLeft="13dp"
            android:textSize="@dimen/sp_14"
            android:layout_height="wrap_content"/>
        <TextView
            android:id="@+id/tv3"
            android:layout_width="wrap_content"
            android:textStyle="bold"
            android:textColor="@color/c_8a91a0"
            app:layout_constraintTop_toBottomOf="@id/tv22"
            app:layout_constraintLeft_toLeftOf="parent"
            android:text="@string/txt_feedresult"
            android:layout_marginTop="@dimen/dp_11"
            android:textSize="@dimen/sp_14"
            android:layout_height="wrap_content"/>
        <TextView
            android:id="@+id/tv33"
            android:layout_width="0dp"
            android:textStyle="bold"
            android:textColor="@color/white"
            app:layout_constraintTop_toTopOf="@id/tv3"
            app:layout_constraintLeft_toRightOf="@id/tv3"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginLeft="13dp"
            android:text="@string/txt_feedtype"
            android:textSize="@dimen/sp_14"
            android:layout_height="wrap_content"/>
        <TextView
            android:id="@+id/tv4"
            android:layout_width="wrap_content"
            android:textStyle="bold"
            android:textColor="@color/c_8a91a0"
            app:layout_constraintTop_toBottomOf="@id/tv3"
            android:layout_marginTop="@dimen/dp_11"
            app:layout_constraintLeft_toLeftOf="parent"
            android:text="@string/txt_feddtime"
            android:textSize="@dimen/sp_14"
            android:layout_height="wrap_content"/>
        <TextView
            android:id="@+id/tv44"
            android:layout_width="wrap_content"
            android:textStyle="bold"
            android:textColor="@color/white"
            app:layout_constraintTop_toTopOf="@id/tv4"
            app:layout_constraintLeft_toRightOf="@id/tv4"
            android:layout_marginLeft="13dp"
            android:text="@string/txt_feedtype"
            android:textSize="@dimen/sp_14"
            android:layout_height="wrap_content"/>

    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.constraintlayout.widget.ConstraintLayout>